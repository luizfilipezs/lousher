{
  "name": "louscher-admin",
  "version": "1.0.0",
  "description": "Administration platform for Louscher (wine e-commerce)",
  "main": "index.js",
  "scripts": {
    "styl": "stylus -w styl/main.styl -o css/main.css",
    "ts": "tsc --watch",
    "browserify": "watchify js/main.js -o js/bundle.js",
    "build": "concurrently --kill-others \"npm run ts\" \"npm run browserify\" \"npm run styl\"",
    "minify": "minify js/bundle.js > js/bundle.min.js"
  },
  "keywords": [
    "wine",
    "e-commerce",
    "admin"
  ],
  "author": "Luiz Filipe da Silva",
  "license": "MIT",
  "dependencies": {
    "http-service-ts": "^1.0.9"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "typescript": "^3.9.5"
  }
}
